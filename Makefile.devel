
develall: man STATUS

man: FORCE
	cd man && $(MAKE)

test: FORCE
	cd test && start_test

SPECTEST_DIR = ./test/release/examples/spec
spectests: FORCE
	rm -rf $(SPECTEST_DIR)
	./util/devel/test/extract_tests -o $(SPECTEST_DIR) spec/*.tex

STATUS: STATUS.devel
	grep -v "^\ *#" STATUS.devel > STATUS


