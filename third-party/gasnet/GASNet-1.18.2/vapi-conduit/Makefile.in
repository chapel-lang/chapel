@BUILD_PARSYNC_LIBS_TRUE@libgasnet_vapi_parsync_a_SOURCES = 
@BUILD_PARSYNC_LIBS_TRUE@libraries_parsync = libgasnet-vapi-parsync.a
libraries = $(libraries_seq) $(libraries_par) $(libraries_parsync)
@USE_VAPI_CONDUIT_TRUE@lib_LIBRARIES = $(libraries) 
