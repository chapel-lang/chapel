				FI_CHECK_PACKAGE([psm2],
						 [psm2.h],
						 [psm2],
						 [psm2_mq_ipeek_dequeue_multi],
						 [],
						 [$psm2_PREFIX],
						 [$psm2_LIBDIR],
						 [psm2_happy=1],
						 [psm2_happy=0])
			      ])
			AS_IF([test x$psm2_happy = x0],
			      [
				$as_echo "$as_me: recheck psm2 without psm2_mq_ipeek_dequeue_multi."
				have_psm2_mq_req_user=0
				FI_CHECK_PACKAGE([psm2],
