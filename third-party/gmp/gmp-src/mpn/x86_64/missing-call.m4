define(`adox',`
	push	$1
	push	$2
	call	__gmp_adox
	pop	$2
')

define(`adcx',`
	push	$1
	push	$2
	call	__gmp_adcx
	pop	$2
')

define(`mulx',`
	push	$1
	call	__gmp_mulx
	pop	$2
	pop	$3
')
