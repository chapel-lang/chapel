@HWLOC_BUILD_DOXYGEN_TRUE@	       done=1; \
@HWLOC_BUILD_DOXYGEN_TRUE@	   fi; \
@HWLOC_BUILD_DOXYGEN_TRUE@	done; \
@HWLOC_BUILD_DOXYGEN_TRUE@	mv letter-refman.pdf $(DOCDIR)/$(DOX_LETTERPDF)

