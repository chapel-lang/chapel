ucontext_t child, parent;
int comparison = 0;
void inout(void *junk) { comparison = (junk == (void*)5); }
int main()
{
	char *stack = malloc(1024*1024*16);
	getcontext(&child);
	getcontext(&parent);
	child.uc_stack.ss_sp = stack + 1024*1024*8;
