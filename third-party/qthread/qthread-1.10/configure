#include <assert.h>
#include <stdlib.h>
#include <ucontext.h>

void func(void * msg)
{
	long expected = 0;
	expected = (1L << 16);
	expected = expected << 16;
	expected += 1L;

	((long)msg == expected) ? exit(0) : exit(1);
}

int main()
{
	if (sizeof(int) == sizeof(void*)) {
		return 0;
	} else {
		ucontext_t c, d;
		/* big enough to ignore stack direction */
		char * stack = malloc(1024*1024*16);
		long msg = 0;

		if (sizeof(long) != sizeof(void*)) return 3;
		getcontext(&c);
		getcontext(&d);
		c.uc_stack.ss_sp = stack + 1024*1024*8;
		c.uc_stack.ss_size = 1024*1024*8 - 64;
		c.uc_link = &d;
		/* keep compiler quiet */
		msg = (1L << 16);
		msg = msg << 16;
		msg += 1L;

		makecontext(&c, (void (*)(void))func, 1, msg);
		swapcontext(&d, &c);

		return 2;
	}
}
