var a = (1, 2, 3, 4);
var b = (-1, 3, 1, 7);
var biga = (1200, 344, 55, -2103);

writeln("Not");
writeln(~a);

writeln();
writeln("And");
writeln(a & b);

writeln();
writeln("Or");
writeln(a | b);

writeln();
writeln("XOr");
writeln(a ^ b);

writeln();
writeln("Left Shift");
writeln(b << a);

writeln();
writeln("Right Shift");
writeln(biga >> a);


