var a:complex;
var b:complex;

a = 1.2 + 3.4i;
b = 5.6 - 7.8i;
writeln( "a = ", a);
writeln( "b = ", b);

var f:real = 2.3;

writeln( "f = ", f);
writeln( "a + f = ", a + f);
writeln( "f + a = ", f + a);
writeln( "a - f = ", a - f);
writeln( "f - a = ", f - a);
writeln( "a * b = ", a * b);
writeln( "a / b = ", a / b);
