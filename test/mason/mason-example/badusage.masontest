mason clean

# not built
mason run --example example.chpl || echo "exited with $?"

# no --build
mason run --example example.chpl --force || echo "exited with $?"

# no --build
mason run --example example.chpl --no-update || echo "exited with $?"

# unknown
mason build --example Unknown || echo "exited with $?"
mason run --example Unknown || echo "exited with $?"
