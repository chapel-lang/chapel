use Math;

var a: real = -0.9999;
var b: real = -1.0/2.0;
var c: real = 0;
var d: real = 1.0/2.0;
var e: real = 0.999999;

writeln("a = ", a);
writeln("atanh(a) = ", atanh(a));
writeln("b = ", b);
writeln("atanh(b) = ", atanh(b));
writeln("c = ", c);
writeln("atanh(c) = ", atanh(c));
writeln("d = ", d);
writeln("atanh(d) = ", atanh(d));
writeln("e = ", e);
writeln("atanh(e) = ", atanh(e));
