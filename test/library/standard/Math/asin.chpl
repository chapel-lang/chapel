use Math;

var a: real = -1;
var b: real = -1.0/2.0;
var c: real = 0.0;
var d: real = 1.0/2.0;
var e: real = 1;

writeln("a = ", a);
writeln("asin(a) = ", asin(a));
writeln("b = ", b);
writeln("asin(b) = ", asin(b));
writeln("c = ", c);
writeln("asin(c) = ", asin(c));
writeln("d = ", d);
writeln("asin(d) = ", asin(d));
writeln("e = ", e);
writeln("asin(e) = ", asin(e));
