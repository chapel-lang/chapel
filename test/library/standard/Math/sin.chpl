use Math;

var pi : real = 3.1415926535;

writeln("pi = ", pi);

writeln("sin(-2*pi) = ", sin(-2*pi));
writeln("sin(-3*pi/2) = ", sin(-3*pi/2));
writeln("sin(-pi) = ", sin(-pi));
writeln("sin(-pi/2) = ", sin(-pi/2));
writeln("sin(0) = ", sin(0.0));
writeln("sin(pi/2) = ", sin(pi/2));
writeln("sin(pi) = ", sin(pi));
writeln("sin(3*pi/2) = ", sin(3*pi/2));
writeln("sin(2*pi) = ", sin(2*pi));
