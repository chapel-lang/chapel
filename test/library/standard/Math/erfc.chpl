use Math;

var a: real = -100;
var b: real = -0.05;
var c: real = 0;
var d: real = 0.05;
var e: real = 100;

writeln("a = ", a);
writeln("erfc(a) = ", erfc(a));
writeln("b = ", b);
writeln("erfc(b) = ", erfc(b));
writeln("c = ", c);
writeln("erfc(c) = ", erfc(c));
writeln("d = ", d);
writeln("erfc(d) = ", erfc(d));
writeln("e = ", e);
writeln("erfc(e) = ", erfc(e));
