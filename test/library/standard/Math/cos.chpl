use Math;

var pi : real = 3.1415926535;

writeln("pi = ", pi);

writeln("cos(-2*pi) = ", cos(-2*pi));
writeln("cos(-3*pi/2) = ", cos(-3*pi/2));
writeln("cos(-pi) = ", cos(-pi));
writeln("cos(-pi/2) = ", cos(-pi/2));
writeln("cos(0) = ", cos(0.0));
writeln("cos(pi/2) = ", cos(pi/2));
writeln("cos(pi) = ", cos(pi));
writeln("cos(3*pi/2) = ", cos(3*pi/2));
writeln("cos(2*pi) = ", cos(2*pi));
