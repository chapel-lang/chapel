use Math;

var a: real = -5.0;
var b: real = -1.0;
var c: real = 0.0;
var d: real = 1.0;
var e: real = 15.0;

writeln("a = ", a);
writeln("cosh(a) = ", cosh(a));
writeln("b = ", b);
writeln("cosh(b) = ", cosh(b));
writeln("c = ", c);
writeln("cosh(c) = ", cosh(c));
writeln("d = ", d);
writeln("cosh(d) = ", cosh(d));
writeln("e = ", e);
writeln("cosh(e) = ", cosh(e));
