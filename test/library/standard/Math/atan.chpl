use Math;

var a: real = -100000000.0;
var b: real = -1.0;
var c: real = 0.0;
var d: real = 1.0;
var e: real = 100000000.0;

writeln("a = ", a);
writeln("atan(a) = ", atan(a));
writeln("b = ", b);
writeln("atan(b) = ", atan(b));
writeln("c = ", c);
writeln("atan(c) = ", atan(c));
writeln("d = ", d);
writeln("atan(d) = ", atan(d));
writeln("e = ", e);
writeln("atan(e) = ", atan(e));
