use Math;

var a: real = 1.0;
var b: real = 1.5;
var c: real = 2.0;
var d: real = 100000.0;
var e: real = 1000000000.0;

writeln("a = ", a);
writeln("acosh(a) = ", acosh(a));
writeln("b = ", b);
writeln("acosh(b) = ", acosh(b));
writeln("c = ", c);
writeln("acosh(c) = ", acosh(c));
writeln("d = ", d);
writeln("acosh(d) = ", acosh(d));
writeln("e = ", e);
writeln("acosh(e) = ", acosh(e));
