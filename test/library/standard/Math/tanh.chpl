use Math;

var a: real = -4;
var b: real = -1.0/2.0;
var c: real = 0;
var d: real = 1.0/2.0;
var e: real = 10;

writeln("a = ", a);
writeln("tanh(a) = ", tanh(a));
writeln("b = ", b);
writeln("tanh(b) = ", tanh(b));
writeln("c = ", c);
writeln("tanh(c) = ", tanh(c));
writeln("d = ", d);
writeln("tanh(d) = ", tanh(d));
writeln("e = ", e);
writeln("tanh(e) = ", tanh(e));
