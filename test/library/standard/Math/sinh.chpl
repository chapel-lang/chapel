use Math;

var a: real = -5.0;
var b: real = -1.0;
var c: real = 0.0;
var d: real = 1.0;
var e: real = 100.0;

writeln("a = ", a);
writeln("sinh(a) = ", sinh(a));
writeln("b = ", b);
writeln("sinh(b) = ", sinh(b));
writeln("c = ", c);
writeln("sinh(c) = ", sinh(c));
writeln("d = ", d);
writeln("sinh(d) = ", sinh(d));
writeln("e = ", e);
writeln("sinh(e) = ", sinh(e));
