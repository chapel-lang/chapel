use Math;

var pi : real = 3.1415926535;

writeln("pi = ", pi);

writeln("tan(-2*pi) = ", tan(-2*pi));
writeln("tan(-3*pi/2) = ", tan(-3*pi/2));
writeln("tan(-pi) = ", tan(-pi));
writeln("tan(-pi/2) = ", tan(-pi/2));
writeln("tan(0) = ", tan(0.0));
writeln("tan(pi/2) = ", tan(pi/2));
writeln("tan(pi) = ", tan(pi));
writeln("tan(3*pi/2) = ", tan(3*pi/2));
writeln("tan(2*pi) = ", tan(2*pi));
