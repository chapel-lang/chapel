writeln(replaceDirname("baz ending.","chapel"));
writeln(replaceDirname("../foo","chapel"));
writeln(replaceDirname(".foo","chapel"));
writeln(replaceDirname("bar/../foo","chapel/../"));
writeln(replaceDirname("/.././bar/../foo","chapel"));
writeln(replaceDirname("", ""));
