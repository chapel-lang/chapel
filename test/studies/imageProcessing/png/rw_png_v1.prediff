#!/bin/sh

outfile=$2

# TODO: find a way to retrieve command-line file names in order to make this
# prediff generic

# Use diff to compare the files
if diff -q "bear_pixel.png" "bear_pixel.good.png" >/dev/null; then
    echo "Test passed: files match!" >> $outfile
else
    echo "Test failed: files do not match!" >> $outfile
fi
