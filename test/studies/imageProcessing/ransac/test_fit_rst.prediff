#!/bin/sh

outfile=$2

# TODO: find a way to retrieve command-line file names in order to make this
# prediff generic

# Use diff to compare the files
if diff -q "ctf1.png" "ctf1_rst.good.png" >/dev/null && diff -q "ctf2.png" "ctf2_rst.good.png" >/dev/null; then
    echo "\nTest passed: files match!" >> $outfile
else
    echo "\nTest failed: files do not match!" >> $outfile
fi
