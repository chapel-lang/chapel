#!/usr/bin/env python3

import sys, os, string

logfile = sys.argv[2]
f = open(logfile, 'r')
lines = f.readlines()
f.close()

if len(lines) == 1:
   if lines[0].find('<command-line arg>') == 0:
      l = lines[0].split(':')
      l[1] = '1'
      newl = ':'.join(l)
      f = open(logfile, 'w')
      f.write(newl)
      f.close()

