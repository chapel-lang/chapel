COMM_LAUNCHER_SRCS = \
        comm-pvm_locales.c \

COMM_SRCS = \
	$(COMM_LAUNCHER_SRCS) \
	comm-pvm.c \

SVN_SRCS = $(COMM_SRCS)
SRCS = $(SVN_SRCS)

COMM_OBJS = \
	$(COMM_SRCS:%.c=$(COMM_OBJDIR)/%.o)

COMM_LAUNCHER_OBJS = \
	$(COMM_LAUNCHER_SRCS:%.c=$(COMM_LAUNCHER_OBJDIR)/%.o)

ifneq ($(MAKE_LAUNCHER), 1)
RUNTIME_CFLAGS += -I$(PVM_ROOT)/include
endif
