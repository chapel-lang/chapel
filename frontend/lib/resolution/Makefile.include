SRCS = $(FRONTEND_RESOLUTION_SRCS)

FRONTEND_RESOLUTION_OBJS = \
	$(FRONTEND_RESOLUTION_SRCS:%.cpp=$(FRONTEND_RESOLUTION_OBJDIR)/%.$(OBJ_SUFFIX))
